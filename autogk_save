--== GUI ==--
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local ToggleJumpE = Instance.new("TextButton")
local ToggleJumpQ = Instance.new("TextButton")
local AutoSaveBtn = Instance.new("TextButton")

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
Frame.Parent = ScreenGui
Frame.Size = UDim2.new(0, 200, 0, 150)
Frame.Position = UDim2.new(0.5, -100, 0.5, -75)
Frame.BackgroundColor3 = Color3.fromRGB(30,30,30)

ToggleJumpE.Name = "JumpE"
ToggleJumpE.Parent = Frame
ToggleJumpE.Size = UDim2.new(0, 180, 0, 40)
ToggleJumpE.Position = UDim2.new(0, 10, 0, 10)
ToggleJumpE.Text = "Jump E: OFF"

ToggleJumpQ.Name = "JumpQ"
ToggleJumpQ.Parent = Frame
ToggleJumpQ.Size = UDim2.new(0, 180, 0, 40)
ToggleJumpQ.Position = UDim2.new(0, 10, 0, 60)
ToggleJumpQ.Text = "Jump Q: OFF"

AutoSaveBtn.Name = "AutoSave"
AutoSaveBtn.Parent = Frame
AutoSaveBtn.Size = UDim2.new(0, 180, 0, 40)
AutoSaveBtn.Position = UDim2.new(0, 10, 0, 110)
AutoSaveBtn.Text = "Auto Save: OFF"

--== Скрипт функционала ==--
local jumpE = false
local jumpQ = false
local autoSave = false

ToggleJumpE.MouseButton1Click:Connect(function()
    jumpE = not jumpE
    ToggleJumpE.Text = "Jump E: " .. (jumpE and "ON" or "OFF")
end)

ToggleJumpQ.MouseButton1Click:Connect(function()
    jumpQ = not jumpQ
    ToggleJumpQ.Text = "Jump Q: " .. (jumpQ and "ON" or "OFF")
end)

AutoSaveBtn.MouseButton1Click:Connect(function()
    autoSave = not autoSave
    AutoSaveBtn.Text = "Auto Save: " .. (autoSave and "ON" or "OFF")
end)

--== Автопрыжок ==--
game:GetService("UserInputService").InputBegan:Connect(function(input, processed)
    if processed then return end
    local character = game.Players.LocalPlayer.Character
    if not character then return end
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    if not humanoid then return end

    if jumpE and input.KeyCode == Enum.KeyCode.E then
        humanoid.Jump = true
    elseif jumpQ and input.KeyCode == Enum.KeyCode.Q then
        humanoid.Jump = true
    end
end)

--== Автосейв ГК ==--
game:GetService("RunService").Stepped:Connect(function()
    if autoSave then
        -- Здесь нужно прописать твой код автосейва, например:
        -- game:GetService("ReplicatedStorage").RemoteSave:FireServer()
    end
end)
